<% if account_signed_in? %>
  welcome <%= current_account.name %>(@<%= current_account.username %>)
  <%= button_to 'Edit Account', edit_account_registration_path, method: 'get' %>
  <%= button_to 'Logout', destroy_account_session_path, method: 'delete' %>
<% else %>
  <%= button_to 'Login', new_account_session_path %>
  <%= button_to 'Register', new_account_registration_path, method: 'get'%>
<% end %>

